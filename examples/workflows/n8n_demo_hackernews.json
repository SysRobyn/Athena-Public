{
    "name": "Athena n8n Demo: Hacker News Scraper",
    "nodes": [
        {
            "parameters": {},
            "name": "Start",
            "type": "n8n-nodes-base.start",
            "typeVersion": 1,
            "position": [
                250,
                300
            ]
        },
        {
            "parameters": {
                "url": "https://news.ycombinator.com/rss",
                "responseFormat": "string",
                "options": {}
            },
            "name": "Fetch Hacker News",
            "type": "n8n-nodes-base.httpRequest",
            "typeVersion": 3,
            "position": [
                450,
                300
            ]
        },
        {
            "parameters": {
                "options": {}
            },
            "name": "Parse XML",
            "type": "n8n-nodes-base.xml",
            "typeVersion": 1,
            "position": [
                650,
                300
            ]
        },
        {
            "parameters": {
                "fieldToSplitOut": "rss.channel.item"
            },
            "name": "Split Items",
            "type": "n8n-nodes-base.itemLists",
            "typeVersion": 1,
            "position": [
                850,
                300
            ]
        },
        {
            "parameters": {
                "maxItems": 5
            },
            "name": "Limit to Top 5",
            "type": "n8n-nodes-base.limit",
            "typeVersion": 1,
            "position": [
                1050,
                300
            ]
        }
    ],
    "connections": {
        "Start": {
            "main": [
                [
                    {
                        "node": "Fetch Hacker News",
                        "type": "main",
                        "index": 0
                    }
                ]
            ]
        },
        "Fetch Hacker News": {
            "main": [
                [
                    {
                        "node": "Parse XML",
                        "type": "main",
                        "index": 0
                    }
                ]
            ]
        },
        "Parse XML": {
            "main": [
                [
                    {
                        "node": "Split Items",
                        "type": "main",
                        "index": 0
                    }
                ]
            ]
        },
        "Split Items": {
            "main": [
                [
                    {
                        "node": "Limit to Top 5",
                        "type": "main",
                        "index": 0
                    }
                ]
            ]
        },
        "Limit to Top 5": {
            "main": [
                []
            ]
        }
    }
}